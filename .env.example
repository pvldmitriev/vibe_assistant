# AI API Configuration

# Основная модель (OpenRouter или OpenAI)
DEFAULT_MODEL=alibaba/tongyi-deepresearch-30b-a3b:free

# OpenRouter API ключ (для бесплатных моделей)
# Получите на https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_key_here

# OpenAI API ключ (опционально, если используете OpenAI модели)
# Получите на https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Для OpenAI моделей (если не используете DEFAULT_MODEL)
AI_MODEL=gpt-4

# Максимальное количество токенов для генерации
MAX_TOKENS=64000

# Backend Configuration
BACKEND_PORT=3001
NODE_ENV=development

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001

# Telegram Bot Configuration
# Получите токен у @BotFather в Telegram
BOT_TOKEN=your_telegram_bot_token_here
